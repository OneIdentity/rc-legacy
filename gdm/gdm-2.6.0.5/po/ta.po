#: daemon/gdm.c:364
#, c-format
msgid "%s: BaseXsession empty, using %s/gdm/Xsession"
msgstr "%s: அடிப்படைXஅமர்வு காலி,  %s/gdm/Xஅமர்வை பயன்படுத்துகிறது"

#: daemon/gdm.c:408
#, c-format
msgid "%s: Standard X server not found, trying alternatives"
